import pandas as pd

pd.options.display.max_rows = 3

df = pd.read_csv('work_with_.csv')
strr = df.to_string()
print("\n\n----- strr\n")
print(strr)
print("\n\n----- head-4\n")
print(df.head(4))
print("\n\n----- tail-4\n")
print(df.tail(4))
print("\n\n----- info\n")
print(df.info(3))
print("\n----- end\n")
